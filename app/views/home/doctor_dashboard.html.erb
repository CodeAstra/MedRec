<div class="col-md-6">
  <h2>Upcoming Appointments</h2>
  <div class="well">  
    <% @next_appointments.each do |consultation|%>
      <h2><%= consultation.patient_profile.user.name %></h2>
      <h3><%= consultation.patient_profile.unique_id %></h3>
      <p><%= consultation.appointment_date %></p>
      <p><%= consultation.comments%></p>
    <%end%>
    <%= link_to "Setup New Appointment" , new_consultation_path, class: "btn btn-primary" %>
  </div>
</div>
<div class="col-md-6">
  <h2>Search the patient</h2>
  <p>
    <input type="text">   
  </p>
  <button class= "btn btn-primary">Search</button>

</div>